extends layout_student

block content
    h1= title
    div(align = 'right')
        span.badge.badge-primary
            p(style='font-size:18px') 论文专利成绩: #{score}
    br
    each output in output_list
        .card
            .card-body
                h4.card-title #{output.name}
            .card-footer.p-0.text-center
                .card-footer-item.card-footer-item-bordered
                    a.card-link(href=output.url) 项目详情
    else
        p 无相关信息

    p
        a(href='/catalog/academic_outputs/create')
            button.btn.btn-primary 提交添加项目